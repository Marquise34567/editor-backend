{
  "name": "auto-editor-pro-backend",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "prisma migrate deploy && node dist/index.js",
    "postinstall": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "prisma:seed": "ts-node prisma/seed.ts",
    "env:wizard": "ts-node scripts/env-wizard.ts",
    "add:supabase": "ts-node scripts/add-supabase.ts",
    "add:db": "ts-node scripts/add-db.ts",
    "add:stripe": "ts-node scripts/add-stripe.ts",
    "setup": "ts-node scripts/setup.ts",
    "verify": "ts-node scripts/verify.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "@supabase/supabase-js": "^2.14.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.3",
    "prisma": "^5.0.0",
    "node-fetch": "^3.3.2",
    "stripe": "^12.0.0",
    "zod": "^3.23.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
